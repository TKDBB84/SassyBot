{
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "node16",
    "target": "es2022",
    "types": ["node"],
    "strictNullChecks": true,
    "moduleResolution": "node16",
    "declaration": false,
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
